FR GA(4,1)   ! sat-USE
FR GA(4,2)   ! Dostoso-USE
FR GA(4,3)   ! ZLOZO-USE

FR BE(4,1)   ! korzbzp ->USE
FR BE(4,2)   ! korzysci->USE
FR BE(4,3)   ! Koszty->USE

