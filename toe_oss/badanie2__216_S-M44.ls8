LE ; koszty korzysci korzbzp use
LK ; satysfakcja dostoso zlozonosc

FR LX(2,1)     ! sat=2 / dostoso=1
FR LX(5,3)     ! zlozo

FR LY(2,1)     ! koszty
FR LY(4,2)     ! korzysci
