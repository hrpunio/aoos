! Okre¶lenie modelu SEM (PS=DI,FR)
MO NY=7 NE=4  NX=5 NK=3  LX=FU,FI LY=FU,FI BE=FU,FI GA=FU,FI  PH=SY,FR    PS=DI,FR TD=DI,FR TE=DI,FR

